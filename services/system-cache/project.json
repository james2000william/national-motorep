{
  "name": "system-cache",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/system-cache/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "options": {
        "bundle": true,
        "sourcemap": true,
        "minify": false,
        "main": "services/system-cache/src/index.ts",
        "tsConfig": "services/system-cache/tsconfig.app.json",
        "format": ["esm"],
        "target": "esnext",
        "conditions": ["worker", "browser"],
        "outputPath": "services/system-cache/dist",
        "outExtension": { ".js": ".mjs" }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "wrangler dev --local"
          }
        ],
        "cwd": "services/system-cache"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "wrangler publish"
          }
        ],
        "cwd": "services/system-cache"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/system-cache/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/services/system-cache"],
      "options": {
        "jestConfig": "services/system-cache/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
