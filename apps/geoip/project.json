{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "geoip",
  "sourceRoot": "apps/geoip/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/geoip",
        "main": "apps/geoip/src/index.ts",
        "tsConfig": "apps/geoip/tsconfig.app.json",
        "assets": ["apps/geoip/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/geoip/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/geoip/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "wrangler dev --local"
          }
        ],
        "cwd": "apps/geoip"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "wrangler publish"
          }
        ],
        "cwd": "apps/geoip"
      }
    }
  },
  "tags": []
}
