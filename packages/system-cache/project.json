{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/system-cache/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/system-cache",
        "main": "packages/system-cache/src/index.ts",
        "tsConfig": "packages/system-cache/tsconfig.lib.json",
        "assets": ["packages/system-cache/*.md"]
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn wrangler dev --local"
          }
        ],
        "cwd": "packages/system-cache"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/system-cache/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/system-cache"],
      "options": {
        "jestConfig": "packages/system-cache/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
