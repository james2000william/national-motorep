{
  "name": "shared-lang",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/lang/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/lang",
        "main": "packages/shared/lang/src/index.ts",
        "tsConfig": "packages/shared/lang/tsconfig.lib.json",
        "assets": ["packages/shared/lang/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/lang/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/shared/lang/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
