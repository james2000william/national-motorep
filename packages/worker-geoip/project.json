{
  "name": "worker-geoip",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/worker-geoip/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/worker-geoip",
        "main": "packages/worker-geoip/src/index.ts",
        "tsConfig": "packages/worker-geoip/tsconfig.lib.json",
        "assets": ["packages/worker-geoip/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/worker-geoip/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/worker-geoip"],
      "options": {
        "jestConfig": "packages/worker-geoip/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "start": {
    "executor": "nx:run-commands",
    "options": {
      "commands": [
        {
          "command": "yarn wrangler dev --local"
        }
      ],
      "cwd": "packages/worker-geoip"
    }
  },
  "tags": []
}
